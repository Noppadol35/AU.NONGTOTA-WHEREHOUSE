      "config": {
        "distDir": "frontend/.next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://au-nongtota-wherehouse-e3c4dkaxu-noppadol-35.vercel.app"
  }
}
